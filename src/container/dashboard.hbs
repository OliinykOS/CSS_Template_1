<html>
  <head>
    <title>
      Dashboard page
    </title>

    <meta charset='utf-8' />

    <link
      rel='apple-touch-icon'
      sizes='180x180'
      href='fav/apple-touch-icon.png'
    />
    <link
      rel='icon'
      type='image/png'
      sizes='32x32'
      href='fav/favicon-32x32.png'
    />
    <link
      rel='icon'
      type='image/png'
      sizes='16x16'
      href='fav/favicon-16x16.png'
    />
    <link rel='manifest' href='/fav/site.webmanifest' />
    <link
      rel='mask-icon'
      href='fav/safari-pinned-tab.svg'
      color='#5bbad5'
    />
    <meta
      name='msapplication-TileColor'
      content='#da532c'
    />
    <meta name='theme-color' content='#ffffff' />

    <link href='css/dashboard.css' rel='stylesheet' />

    <meta
      name='viewport'
      content='viewport-fit=cover, width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no'
    />
  </head>

  <body>
    <div class='page page--dashboard'>
      <aside
        class='page__aside aside vertical vertical--big'
      >
        <div
          class='aside__logo text text--big'
        >Sidebar</div>
        <nav class='vertical vertical--small'>
          <a
            href='#'
            class='nav-button nav-button--active click'
          >
            <span
              class='icon nav-button__icon'
              style="background-image: url('/svg/nav/home.svg');"
            ></span>
            <span class='text text--light'>Home</span>
          </a>

          <a href='#' class='nav-button click'>
            <span
              class='icon nav-button__icon'
              style="background-image: url('/svg/nav/orders.svg');"
            ></span>
            <span class='text text--light'>Orders</span>
          </a>

          <a href='#' class='nav-button click'>
            <span
              class='icon nav-button__icon'
              style="background-image: url('/svg/nav/products.svg');"
            ></span>
            <span class='text text--light'>Products</span>
          </a>

        </nav>
      </aside>
      <header class='header text'>
        <div class='layout header__content text--small'>
          <div class='header__search relative'>
            <input
              type='text'
              placeholder='Search'
              class='text field field--header'
            />
            <span
              class='icon header__icon-search'
              style="background-image: url('/svg/header/search.svg')"
            ></span>
            <span
              class='icon icon--header-settings icon--button icon--color-blue click'
              style="background-image: url('/svg/header/settings.svg')"
            ></span>
          </div>
          <div class='header__buttons'>
            <span
              class='icon icon--header-buttons icon--button icon--color-yellow-light click'
              style="background-image: url('/svg/header/antenna-signal.svg')"
            ></span>
            <span
              class='icon icon--header-buttons icon--button icon--color-blue-light click'
              style="background-image: url('/svg/header/translate.svg')"
            ></span>
            <span
              class='icon icon--header-buttons icon--button icon--color-yellow-light click'
              style="background-image: url('/svg/header/bell.svg')"
            ></span>
          </div>
        </div>
      </header>
      <main class='main'>
        <div class='layout'>
          <section class='box analytic__block'>
            <div
              class='analytic analytic--double analytic--purple'
            >
              <span
                class='icon analytic__button icon--button icon--color-purple-dark click'
                style="background-image: url('/svg/analytic/money.svg')"
              ></span>
              <div
                class='relative vertical vertical--small'
              >
                <div
                  class='analytic__title text text--light'
                >$500.00</div>
                <div
                  class='text text--small text--lighter'
                >Total earning</div>
              </div>
              <span
                class='icon analytic__arrow'
                style="background-image: url('/svg/analytic/blue-arrow.svg')"
              ></span>

            </div>
            <div
              class='analytic analytic--double analytic--blue'
            >
              <span
                class='icon analytic__button icon--button icon--color-blue click'
                style="background-image: url('/svg/analytic/shopping-bag.svg')"
              ></span>
              <div
                class='relative vertical vertical--small'
              >
                <div
                  class='analytic__title text text--light'
                >$961</div>
                <div
                  class='text text--small text--lighter'
                >Total Order</div>
              </div>
            </div>
            <div class='analytic analytic--blue'>
              <span
                class='icon analytic__button icon--button icon--color-blue click'
                style="background-image: url('/svg/analytic/money.svg')"
              ></span>
              <div
                class='relative vertical vertical--extra-small'
              >
                <div
                  class='analytic__title-small text text--light'
                >$203k</div>
                <div
                  class='text text--smaller text--lighter'
                >Total Income</div>
              </div>
            </div>
            <div class='analytic analytic--white'>
              <span
                class='icon analytic__button icon--button icon--color-yellow-light click'
                style="background-image: url('/svg/analytic/shop.svg')"
              ></span>
              <div
                class='relative vertical vertical--extra-small'
              >
                <div
                  class='analytic__title-small text'
                >$203k</div>
                <div
                  class='text text--smaller text--gray'
                >Total Income</div>
              </div>
            </div>
          </section>
          <div class='main_info__block'>
            <section class='box purchase__block'>
              <div
                class='text text--big text--bold'
              >Purchase list</div>
              <div class='divider'></div>
              <div class='purchase_item'>
                <div class='purchase_element'>
                  <div
                    class='text text--small'
                  >ID437661</div>
                  <div class='text text--small'>May 11,
                    2023, 14:21</div>
                </div>
                <div class='purchase_element'>
                  <div class='text text--small'>AirOn ProCam</div>
                  <div class='text text--small'>7/8 USB
                    Type-C</div>
                </div>
                <div class='purchase_element'>
                  <div class='text text--small'>132.32 EUR</div>
                  <div
                    class='text text--small text--warning'
                  >Not paid</div>
                </div>
                <div class='purchase_element'>
                  <div
                    class='text text--small'
                  >Customer</div>
                  <div
                    class='text text--small'
                  >robin@com.de</div>
                </div>
              </div>
              <div class='divider'></div>
              <div class='purchase_item'>
                <div class='purchase_element'>
                  <div
                    class='text text--small'
                  >ID437661</div>
                  <div class='text text--small'>May 11,
                    2023, 14:21</div>
                </div>
                <div class='purchase_element'>
                  <div class='text text--small'>PHILIPS
                    Essential</div>
                  <div
                    class='text text--small'
                  >HD9200/90</div>
                </div>
                <div class='purchase_element'>
                  <div class='text text--small'>543 USD</div>
                  <div
                    class='text text--small text--success'
                  >Pay by Stripe</div>
                </div>
                <div class='purchase_element'>
                  <div
                    class='text text--small'
                  >Customer</div>
                  <div
                    class='text text--small'
                  >dima@com.ua</div>
                </div>
              </div>
              <div class='divider'></div>
            </section>
            <section class='box form__block'>
              <div
                class='text text--big text--bold text--dark'
              >Payment</div>
              <div class='divider'></div>
              <div class='radio_block'>
                <div class='radio_element'>
                  <input type='radio' checked='checked' />
                  <label class='text text--dark'>Credit card</label>
                </div>
                <div class='radio_element'>
                  <input type='radio' />
                  <label class='text text--dark'>Credit card</label>
                </div>
                <div class='radio_element'>
                  <input type='radio' />
                  <label
                    class='text text--dark'
                  >PayPal</label>
                </div>

              </div>
              <div class='card__block'>
                <div class='card_field'>
                  <div
                    class='text text--line text--dark'
                  >Name on card</div>
                  <input type='text' class='text field' />
                </div>
                <div class='card_field'>
                  <div
                    class='text text--line text--dark'
                  >Credit card number</div>
                  <input type='text' class='text field' />
                </div>
                <div class='card_field'>
                  <div
                    class='text text--line text--dark'
                  >Expiration</div>
                  <input type='text' class='text field' />
                </div>
                <div class='card_field'>
                  <div
                    class='text text--line text--dark'
                  >CVV</div>
                  <input type='text' class='text field' />
                </div>
              </div>
              <div class='divider'></div>
              <div class='address__block'>
                <div class='card_field'>
                  <div
                    class='text text--line text--dark'
                  >Country</div>
                  <div class='relative click'>
                    <div class='field field--select'>
                      <span
                        class='text text--dark'
                      >Choose...</span>
                    </div>
                    <select class='field__select'>
                      <option value='dog'>Dog</option>
                      <option value='cat'>Cat</option>
                      <option
                        value='hamster'
                      >Hamster</option>
                    </select>
                  </div>

                </div>
                <div class='card_field'>
                  <div
                    class='text text--line text--dark'
                  >State</div>
                  <div class='relative click'>
                    <div class='field field--select'>
                      <span
                        class='text text--dark'
                      >Choose...</span>
                    </div>
                    <select class='field__select'>
                      <option value='dog'>Dog</option>
                      <option value='cat'>Cat</option>
                      <option
                        value='hamster'
                      >Hamster</option>
                    </select>
                  </div>

                </div>
              </div>
              <div class='divider'></div>
              <a
                href='#'
                class='checkout-button text click'
              >
                Continue to checkout
              </a>

            </section>
          </div>
        </div>
      </main>
    </div>
  </body>

</html>